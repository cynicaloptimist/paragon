{
  "rules": {
    "users": {
      "$uid": {
        ".write": "$uid === auth.uid",
        ".read": "$uid === auth.uid",
        "playerViewState": {
          ".read": true
        }
      }
    },
    "playerViews": {
      "$playerViewId": {
        ".read": true,
        ".write": true,
        ".validate": "newData.isString()"
      }
    }
  }
}
